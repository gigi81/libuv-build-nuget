<?xml version="1.0" encoding="utf-8"?>
<package>
    <metadata>
        <id>libuv-shared</id>
        <version>0.0.1-ignored</version>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>libuv shared library packge with native runtime for Windows (x64/x86/arm/arm64), Linux (x64/arm64/arm/armel) and Osx (x64/arm64). CHeck the release notes for more details</description>
        <authors>Luigi Grilli</authors>
        <license type="file">LICENSE</license>
        <releaseNotes></releaseNotes>
        <projectUrl>https://github.com/gigi81/libuv-build-nuget</projectUrl>
        <repository type="git" url="https://github.com/gigi81/libuv-build-nuget.git" />
    </metadata>
    <files>
        <file src="runtimes\win-arm\*.*" target="runtimes\win-arm\native" />
        <file src="runtimes\win-arm64\*.*" target="runtimes\win-arm64\native" />
        <file src="runtimes\win-x86\*.*" target="runtimes\win-x86\native" />
        <file src="runtimes\win-x64\*.*" target="runtimes\win-x64\native" />
        <file src="runtimes\linux-x64\*.*" target="runtimes\linux-x64\native" />
        <file src="runtimes\linux-arm64\*.*" target="runtimes\linux-arm64\native" />
        <file src="runtimes\linux-arm\*.*" target="runtimes\linux-arm\native" />
        <file src="runtimes\linux-armel\*.*" target="runtimes\linux-armel\native" />
        <file src="runtimes\osx-x64\*.*" target="runtimes\osx-x64\native" />
        <file src="runtimes\osx-arm64\*.*" target="runtimes\osx-arm64\native" />
        <file src="libuv\LICENS*" target="" />
    </files>
</package>
